{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": [
      "es2019",
      "dom"
    ],
    "allowJs": true,
    "jsx": "react-native",
    "sourceMap": true,
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@*": ["src/*"]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
}
